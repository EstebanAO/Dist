program factIterative;

fun factorial(a:int) : int {
    var i, res:int;
    i = 1;
    res = 1;
    while (i <= a) {
        res = res * i;
        i = i + 1;
    };
    return res;
}

main {
    var fact : int;
    print("Ingresa el numero para obtener su factorial iterativamente: ");
    read(fact);
    println("El factorial de ", fact, " es: ", factorial(fact));
}
